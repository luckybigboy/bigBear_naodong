/**app.wxss**/

.h1, .h2, .h3, .h4, .h5, .h6, .p, .div, .ul, .li {
  display: block;
}

button::after {
  border: none;
}

.default {
  text-align: center;
  padding-top: 152rpx;
}

.default image {
  width: 380rpx;
  height: 240rpx;
  margin-bottom: 88rpx;
}

.default view {
  font-size: 32rpx;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 44rpx;
  letter-spacing: 2rpx;
}

.default view text {
  color: #f5a623;
  margin-left: 20rpx;
}

.login {
  text-align: center;
  position: fixed;
  width: 100%;
  height: 100%;
  background: #fff;
  z-index: 1000;
  left: 0;
  top: 0;
}

.login image {
  width: 420rpx;
  height: 512rpx;
  display: block;
  margin: 0 auto;
  margin-top: 200rpx;
  margin-bottom: 50rpx;
}

.login text {
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
  line-height: 40rpx;
}

.login button {
  width: 380rpx;
  height: 64rpx;
  display: block;
  color: #fff;
  line-height: 64rpx;
  text-align: center;
  background: #f5a623;
  border-radius: 8rpx;
  margin-top: 16rpx;
  margin-bottom: 30rpx;
}

