<!--pages/index/follow-people/follow-people.wxml-->

<view>
  <view wx:for="{{followList}}" wx:key="{{index}}" class='list'>
    <image src='{{item.user_image}}' class='headimg'></image>
    <text class='userName'>{{item.user_name}}</text>
    <text space='emsp' class='num'>共有{{item.article_num}}条新脑洞 ></text>
  </view>
</view>
